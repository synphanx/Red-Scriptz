   Name = "Spawn Red Fox",
   Callback = function()
    Spawner.SpawnPet("Red Fox", 2.28, 1) 
   -- The function that takes place when the button is pressed
   end,
})

local Tab = Window:CreateTab("Misc", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Miscellaneous (W.I.P.)")
